{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/core/(.*)": "<rootDir>/../libs/core/src/$1",
    "@app/core": "<rootDir>/../libs/core/src",
    "@app/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@app/common": "<rootDir>/../libs/common/src",
    "@app/python/(.*)": "<rootDir>/../libs/python/src/$1",
    "@app/python": "<rootDir>/../libs/python/src",
    "@app/java/(.*)": "<rootDir>/../libs/java/src/$1",
    "@app/java": "<rootDir>/../libs/java/src",
    "@app/gcc/(.*)": "<rootDir>/../libs/gcc/src/$1",
    "@app/gcc": "<rootDir>/../libs/gcc/src"
  }
}